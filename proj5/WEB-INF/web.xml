<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <description>proj5 servlet application</description>
    <display-name>proj5</display-name>
    <servlet>
        <description>GET - ouput selected RSS elements as XHTML</description>
        <servlet-name>Feeds</servlet-name>
        <servlet-class>servlets.Feeds</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Feeds</servlet-name>
        <url-pattern>/feeds</url-pattern>
    </servlet-mapping>
</web-app>
